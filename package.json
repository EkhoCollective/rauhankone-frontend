{
	"name": "rauhankone-frontend",
	"version": "0.0.2",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"gh-pages": "npm run build && npx gh-pages -d build --cname peacemachine.eu",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.2",
		"@eslint/js": "^9.33.0",
		"@fontsource/roboto": "^5.2.6",
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/adapter-static": "^3.0.9",
		"@sveltejs/kit": "^2.36.1",
		"@sveltejs/package": "^2.5.0",
		"@sveltejs/vite-plugin-svelte": "^5.1.1",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.12",
		"@testing-library/jest-dom": "^6.8.0",
		"@testing-library/svelte": "^5.2.8",
		"@types/howler": "^2.2.12",
		"@types/three": "^0.176.0",
		"eslint": "^9.33.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.11.0",
		"gh-pages": "^6.3.0",
		"globals": "^16.3.0",
		"jsdom": "^26.1.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"publint": "^0.3.12",
		"svelte": "^5.38.2",
		"svelte-check": "^4.3.1",
		"tailwindcss": "^4.1.12",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.40.0",
		"vite": "^6.3.5",
		"vitest": "^3.2.4"
	},
	"keywords": [
		"svelte"
	],
	"dependencies": {
		"@dimforge/rapier3d-compat": "^0.18.2",
		"@theatre/core": "^0.7.2",
		"@theatre/studio": "^0.7.2",
		"@threlte/core": "^8.1.4",
		"@threlte/extras": "^9.4.5",
		"@threlte/rapier": "^3.1.5",
		"@threlte/studio": "^0.1.8",
		"@threlte/theatre": "^3.0.7",
		"bits-ui": "^2.9.4",
		"dompurify": "^3.2.6",
		"esbuild": "^0.25.9",
		"esm-env": "^1.2.2",
		"howler": "^2.2.4",
		"install": "^0.13.0",
		"lucide-svelte": "^0.503.0",
		"pnpm": "^10.15.0",
		"svelte-i18n": "^4.0.1",
		"svelte-sound": "^0.6.0",
		"three": "^0.176.0",
		"threlte-postprocessing": "^0.0.10"
	}
}
